{
    "name": "diviky/serverless",
    "description": "The Laravel Serverless CLI",
    "keywords": [
        "laravel",
        "vapor"
    ],
    "homepage": "https://github.com/diviky/serverless",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar@diviky.com"
        }
    ],
    "bin": [
        "deployer"
    ],
    "require": {
        "laravel/vapor-cli": "^1.54"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Diviky\\Serverless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diviky\\Serverless\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
