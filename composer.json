{
    "name": "diviky/serverless",
    "description": "The Laravel Serverless CLI",
    "keywords": [
        "laravel",
        "vapor"
    ],
    "homepage": "https://github.com/diviky/serverless",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar@diviky.com"
        }
    ],
    "bin": [
        "vapor"
    ],
    "require": {
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "laravel/vapor-cli": "^1.10",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "ramsey/uuid": "^3.8|^4.0",
        "symfony/console": "^4.2|^5.0",
        "symfony/process": "^4.2|^5.0",
        "symfony/var-dumper": "^4.2|^5.0",
        "symfony/yaml": "^4.2|^5.0",
        "vlucas/phpdotenv": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Diviky\\Serverless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diviky\\Serverless\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
